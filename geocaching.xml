<ruleset name="GeoCaching">
  <target host="geocaching.com" />
  <target host="*.geocaching.com" />

  <rule from="^http://(www\.)?geocaching\.com/" to="https://www.geocaching.com/"/>
  <rule from="^http://([^/:@]*)\.geocaching\.com/" to="https://$1.geocaching.com/"/>
<!--  <securecookie host=".*\.geocaching\.com$" name="userid" /> -->
</ruleset>
